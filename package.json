{
  "name": "natours",
  "version": "1.0.0",
  "description": "lading page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass scss/index.scss public/css/index.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass scss/index.scss public/css/index.comp.css",
    "concat:css": "concat -o public/css/index.concat.css public/css/icon-font.css public/css/index.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" public/css/index.concat.css -o public/css/index.prefix.css",
    "compress:css": "node-sass public/css/index.prefix.css public/css/index.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Reynan Delizo",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "postcss-cli": "^9.1.0"
  }
}
